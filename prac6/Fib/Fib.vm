// Contains a function Fib.fib that calculates and returns the nth Fibonacci number recursively
// n is supplied as an argument

// Put your code here.
function Fib.fib 0
push argument 0
push constant 2
lt
if-goto smallTwo
goto bigTwo

label smallTwo
    push argument 0 
    return

label bigTwo
    push argument 0 
    push constant 2
    sub
    call Fib.fib 1
    push argument 0
    push constant 1
    sub 
    call Fib.fib 1
    add
    return

